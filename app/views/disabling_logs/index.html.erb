<h1>Listing disabling_logs</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Url</th>
    <th>Button count</th>
    <th>Button type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @disabling_logs.each do |disabling_log| %>
  <tr>
    <td><%= disabling_log.title %></td>
    <td><%= disabling_log.url %></td>
    <td><%= disabling_log.button_count %></td>
    <td><%= disabling_log.button_type %></td>
    <td><%= link_to 'Show', disabling_log %></td>
    <td><%= link_to 'Edit', edit_disabling_log_path(disabling_log) %></td>
    <td><%= link_to 'Destroy', disabling_log, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Disabling log', new_disabling_log_path %>
